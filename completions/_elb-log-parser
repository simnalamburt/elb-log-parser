#compdef elb-log-parser

autoload -U is-at-least

_elb-log-parser() {
    typeset -A opt_args
    typeset -a _arguments_options
    local ret=1

    if is-at-least 5.2; then
        _arguments_options=(-s -S -C)
    else
        _arguments_options=(-s -C)
    fi

    local context curcontext="$curcontext" state line
    _arguments "${_arguments_options[@]}" : \
'-t+[Type of load balancer]:TYPE:(alb classic-lb)' \
'--type=[Type of load balancer]:TYPE:(alb classic-lb)' \
'--skip-parse-errors[Skip parsing errors]' \
'-h[Print help]' \
'--help[Print help]' \
'-V[Print version]' \
'--version[Print version]' \
':path -- Path of directory containing load balancer logs. To read from stdin, use "-":_files -/' \
&& ret=0
}

(( $+functions[_elb-log-parser_commands] )) ||
_elb-log-parser_commands() {
    local commands; commands=()
    _describe -t commands 'elb-log-parser commands' commands "$@"
}

if [ "$funcstack[1]" = "_elb-log-parser" ]; then
    _elb-log-parser "$@"
else
    compdef _elb-log-parser elb-log-parser
fi
